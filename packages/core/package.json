{
  "name": "lz-element",
  "version": "1.0.2",
  "description": "Componets library for lz-element",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "build": "run-p build-umd build-es",
    "build:watch": "run-p build-umd:watch build-es:watch",
    "build-umd": "vite build --config build/vite.umd.config.ts",
    "build-es": "vite build --config build/vite.es.config.ts",
    "build-umd:watch": "vite build --watch --config build/vite.umd.config.ts",
    "build-es:watch": "vite build --watch --config build/vite.es.config.ts",
    "release": "release-it"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "@lz-element/components": "workspace:*",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "vite-plugin-compression2": "^2.2.0"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  }
}
